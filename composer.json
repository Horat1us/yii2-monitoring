{
  "name": "horat1us/yii2-monitoring",
  "description": "Yii2 Monitoring",
  "type": "library",
  "require": {
    "php": "^7.2",
    "yiisoft/yii2": "^2.0.15"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "squizlabs/php_codesniffer": "^3.3",
    "yiisoft/yii2-queue": "^2.1"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Horat1us\\Yii\\Monitoring\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Horat1us\\Yii\\Monitoring\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "lint": "./vendor/bin/phpcs --standard=PSR2 ./src ./tests",
    "phpcbf": "./vendor/bin/phpcbf --standard=PSR2 ./src ./tests",
    "test": "./vendor/bin/phpunit"
  }
}
